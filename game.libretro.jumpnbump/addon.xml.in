<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.jumpnbump"
		name="Jump'n'Bump"
		version="0.1"
		provider-name="">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms></platforms>
		<extensions>zip</extensions>
		<supports_vfs>false</supports_vfs>
		<supports_standalone>true</supports_standalone>
		<requires_opengl>false</requires_opengl>
	</extension>
	<extension point="xbmc.addon.metadata">
		<license>GPLv2</license>
		<platform>@PLATFORM@</platform>
		<source>https://github.com/hoditohod/game.libretro.jumpnbump</source>
		<assets>
			<icon>resources/jumpnbump.png</icon>
			<fanart></fanart>
			<screenshot>resources/screenshot.png</screenshot>
		</assets>
		<summary lang="en_GB">Jump'n'Bump</summary>
	</extension>
</addon>
